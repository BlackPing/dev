<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="socket">  
  <select id="select" resultType="hashmap">
    SELECT `NO`, `TITLE`
	FROM TOPIC
	WHERE DELYN = 'N'
  </select>
</mapper>